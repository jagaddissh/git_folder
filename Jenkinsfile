pipeline {
  agent any
  stages {
    stage('first_stage') {
      steps {
        sh 'echo "jenkins"'
      }
    }

    stage('second_stage') {
      steps {
        sh 'echo "docker"'
      }
    }

    stage('third_stage') {
      steps {
        sh 'echo "kubernetes"'
      }
    }

    stage('fourth_stage') {
      steps {
        sh 'echo "ansible"'
      }
    }

    stage('fifth-stage') {
      steps {
        sh 'echo "terraform"'
      }
    }

  }
}